{
  "extends": "../../tslint.json",
  "rules": {
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      [
        "rs",
        "ngx"
      ],
      [
        "kebab-case",
        "camelCase"
      ]
    ],
    "directive-selector": [
      true,
      "attribute",
      [
        "rs",
        "ngx"
      ],
      "camelCase"
    ],
    "use-host-property-decorator": false
  }
}
