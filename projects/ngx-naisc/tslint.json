{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "rs",
        "ngx"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      [
        "rs",
        "ngx]",
        [
          "kebab-case",
          "camelCase"
        ]
      ]
    ]
  }
}
