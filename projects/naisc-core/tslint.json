{
  "extends": "../../tslint.json",
  "rules": {
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      "naisc",
      [
        "kebab-case",
        "camelCase"
      ]
    ],
    "directive-selector": [
      true,
      "attribute",
      "naisc",
      "camelCase"
    ],
    "use-host-property-decorator": false
  }
}
